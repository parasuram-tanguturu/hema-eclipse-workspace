package com.oops.bike;

public class MotorBike {

	private int speed;

	// behaviour
	// input = int speed

	// output= void

	// name= set spped

	void setSpeed(int speed) {
		if (speed > 0) {
			this.speed = speed;
		}
	}

	void start() {
		System.out.println("Motor Bik startinge");
	}

	int getSpeed() {
		return speed;
	}

	void increaseSpeed(int howMuch) {
		this.speed += howMuch;

	}
	void decreaseSpeed(int howMuch) {
		this.speed=this.speed-howMuch;
		
	}
}
